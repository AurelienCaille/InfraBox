{
    "version": 1,
    "jobs": [{
        "type": "workflow",
        "name": "test",
        "infrabox_file": ".infrabox/output/tests.json"
    }, {
        "type": "workflow",
        "name": "deploy",
        "infrabox_file": ".infrabox/output/deployments.json",
        "depends_on": ["test"]
    }, {
        "type": "workflow",
        "name": "e2e",
        "infrabox_file": ".infrabox/output/e2e.json",
        "depends_on": ["deploy"]
	}, {
        "type": "git",
        "name": "external",
        "clone_url": "https://github.com/InfraBox/slack.git",
        "commit": "master",
        "depends_on": [{"job": "test", "on": ["*"]}]
    }]
}
